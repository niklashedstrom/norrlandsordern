mixin log(l)
  table.borderless.table.m-0
    thead
      tr
        th(scope='col') Namn
        th(scope='col') Tid
    tbody
      each item in l
        tr
          td
            a.user-link(href=`/users/${item.user._id}`)= item.user.name
            if item.user.badge == 'star-gold'
                img.user-badge(src='/images/star-gold.svg', alt='GuldstjÃ¤rna')
            if item.user.badge == 'star-silver'
                img.user-badge(src='/images/star-silver.svg', alt='SilverstjÃ¤rna')
          td= formatDate(item.created_at)